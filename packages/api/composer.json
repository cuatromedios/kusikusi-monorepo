{
    "name": "kusikusi/api",
    "description": "A description for api.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Cuatromedios",
            "email": "dev@cuatromedios.com"
        }
    ],
    "require": {
        "illuminate/support": "^7.0|^8.0",
        "kusikusi/models": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Kusikusi\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kusikusi\\Api\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kusikusi\\ApiServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
